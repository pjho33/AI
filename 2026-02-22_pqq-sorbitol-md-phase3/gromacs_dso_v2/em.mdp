; em.mdp - Energy Minimization
integrator  = steep
nsteps      = 50000
emtol       = 1000.0
emstep      = 0.01

; Output
nstxout     = 500
nstvout     = 0
nstenergy   = 500
nstlog      = 500

; Neighborsearching
cutoff-scheme   = Verlet
ns_type         = grid
nstlist         = 20
rlist           = 1.2

; Electrostatics
coulombtype     = PME
rcoulomb        = 1.2

; VdW
vdwtype         = Cut-off
rvdw            = 1.2
DispCorr        = EnerPres

; PBC
pbc             = xyz
